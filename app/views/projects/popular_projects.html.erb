<div class="row university_projects">
    <div class="projects_type_header col-md-12">
        <h1 class="col-md-6"><%= current_user.university.name %>内のプロジェクト</h1>
    </div>
</div>
<div class="row popular_projects">
    <div class="projects_type_header col-md-12">
        <div class="projects_type col-md-3">
            <h4>人気のプロジェクト</h4>
        </div>
    </div>
    <div class="project_type_body col-md-12">
      <%= render 'projects/projects', projects: @popular_projects %>
      <%= paginate @popular_projects %>
    </div>
</div>